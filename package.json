{
  "name": "vsts-bump",
  "version": "0.0.12",
  "description": "Lib/CLI for bumping the version of VSTS task files",
  "license": "MIT",
  "main": "lib/index.js",
  "bin": "bin/vsts-bump.js",
  "author": {
    "email": "opensource@swellaby.com",
    "name": "Swellaby",
    "url": "http://swellaby.com"
  },
  "contributors": [
    {
      "email": "opensource@swellaby.com",
      "name": "Caleb Cartwright",
      "url": "https://github.com/calebcartwright"
    }
  ],
  "scripts": {
    "build": "npm run lint && npm run test",
    "clean": "rimraf .testresults .coverage",
    "clean:unit": "rimraf .testresults/unit .coverage/unit",
    "clean:component": "rimraf .testresults/component .coverage/component",
    "clean:functional": "rimraf .testresults/functional .coverage/functional",
    "coverage": "nyc --report-dir .coverage/unit --check-coverage false npm run test:unit",
    "coverage:enforce": "nyc --report-dir .coverage/unit npm run test:unit",
    "coverage:open": "npm run coverage && opn .coverage/unit/index.html",
    "lint": "eslint **/*.js",
    "test": "npm run test:unit",
    "test:all": "npm run test:unit && npm run test:component && npm run test:functional",
    "test:unit": "mocha --opts test/unit/mocha.opts",
    "pretest:unit": "npm run clean:unit",
    "test:component": "mocha --opts test/component/mocha.opts",
    "pretest:component": "npm run clean:component",
    "test:functional": "mocha --opts test/functional/mocha.opts",
    "pretest:functional": "npm run clean:functional",
    "version:tagless": "npm --no-git-tag-version version patch",
    "prepublish:local": "npm run build && npm run version:tagless",
    "publish:local": "npm publish --access public",
    "dev:reset": "npm run clean && node -e \"require('rimraf').sync('node_modules');\" && npm run dev:setup",
    "dev:setup": "npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swellaby/vsts-bump.git"
  },
  "keywords": [
    "VSTS"
  ],
  "bugs": {
    "url": "https://github.com/swellaby/vsts-bump/issues"
  },
  "homepage": "https://github.com/swellaby/vsts-bump",
  "devDependencies": {
    "@swellaby/eslint-config": "^1.1.6",
    "@swellaby/nyc-config": "^1.0.12",
    "chai": "^4.1.2",
    "eslint": "^5.0.0",
    "husky": "^1.0.0-rc.4",
    "mocha": "^5.2.0",
    "mocha-multi-reporters": "^1.1.7",
    "mocha-sonarqube-reporter": "^1.0.1",
    "nyc": "^12.0.2",
    "opn-cli": "^3.1.0",
    "rimraf": "^2.6.2",
    "sinon": "^6.0.0"
  },
  "dependencies": {
    "commander": "^2.15.1",
    "glob": "^7.1.2",
    "semver": "^5.5.0"
  }
}
